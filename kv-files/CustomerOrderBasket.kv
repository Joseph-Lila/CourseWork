<ListItemWithCheckbox>:

    IconLeftWidget:
        icon: root.icon

    RightCheckbox:

<CustomerOrderBasket>:
    container: container

    MDBoxLayout:
        orientation: "vertical"

        MDToolbar:
            title: "SERVICES LIST"
            md_bg_color: [200/255, 35/255, 100/255, 255/255]
            elevation: 30
            right_action_items: [["arrow-left", lambda x: self.parent.parent.go_back()], ["minus", lambda x: self.parent.parent.remove_gui_items()], ["plus", lambda x: self.parent.parent.add_items()], ]

        BoxLayout:

            ScrollView:

                MDList:
                    id: container
